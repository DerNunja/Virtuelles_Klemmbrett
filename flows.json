    {
        "id": "0c2b3a1d5e6f7a8b",
        "type": "function",
        "z": "8149379c027f98bb",
        "name": "Parse Telemetrie (JSON)",
        "func": "// Erwartet JSON wie {timestamp, temperature, vibration, state, good, bad}\nif (flow.get('pause')) return null;\n\nlet obj = msg.payload;\ntry {\n  if (typeof obj === 'string') obj = JSON.parse(obj);\n} catch (e) {\n  node.warn('Ungültiges JSON in Telemetrie');\n  return null;\n}\n\n// Fallback Timestamp\nobj.timestamp = obj.timestamp || new Date().toISOString();\n\n// Zahlentypen normalisieren\n['temperature','vibration','good','bad'].forEach(k=>{\n  if (obj[k] != null) obj[k] = Number(obj[k]);\n});\n\nmsg.telemetry = obj; // volle Daten separiert\nmsg.payload = obj;   // für einfache Widgets\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 530,
        "y": 160,
        "wires": [
